{
  "name": "@cuuki/stenciljs-storybook-lib",
  "version": "0.0.1",
  "description": "Starter project for StencilJS / Storybook integration and export as npm package to use in different frameworks",
  "repository": "git://github.com/Cuuki/stenciljs-storybook-lib.git",
  "author": "Patrick SÃ¶llner",
  "license": "GPL-3.0",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "main": "dist/index.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stenciljs-storybook-lib/stenciljs-storybook-lib.js",
  "files": [
